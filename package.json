{
  "name": "telia-styleguide-poc",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack --colors",
    "build-css": "npm run build-css:components && npm run build-css:custom",
    "build-css:components": "postcss src/components/index.css -o public/bundle.components.css",
    "build-css:custom": "postcss src/client/css/index.css -o public/bundle.custom.css",
    "build-server": "webpack --config webpack-server.config.js",
    "css:components:watch": "nodemon --ext css --watch src/components --exec 'npm run build-css:components'",
    "css:custom:watch": "nodemon --ext css --watch src/client/css --exec 'npm run build-css:custom'",
    "dev": "run-p start-dev-server start-server css:components:watch css:custom:watch",
    "gen": "npm run generate-html-doc && npm run generate-react-doc",
    "generate-html-doc": "babel-node ./scripts/html-docgen.js",
    "generate-react-doc": "react-docgen ./src/components -e ReactExampleProps.js -e index.js -o component-metadata--react.json --pretty",
    "start-server": "babel-node src/server/server.js",
    "start-dev-server": "webpack-dev-server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Telia Norge",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "postcss-cli": "^4.0.0",
    "postcss-easy-import": "^2.0.0",
    "postcss-loader": "^2.0.5",
    "postcss-nested": "^2.0.2",
    "react-docgen": "^2.15.0",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "express": "^4.15.2",
    "htmlencode": "0.0.4",
    "lodash": "^4.17.4",
    "marked": "^0.3.6",
    "pretty": "^2.0.0",
    "proxy-middleware": "^0.15.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-element-to-jsx-string": "^7.0.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-router-dom": "^4.1.1"
  }
}
