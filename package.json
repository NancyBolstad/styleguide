{
  "name": "telia-styleguide-poc",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && webpack --config webpack-prod.config.js && npm run build-css && npm run build-server",
    "build-css": "npm run build-css:components && npm run build-css:custom",
    "build-css:components": "postcss src/components/index.css -o public/css/bundle.components.css",
    "build-css:custom": "postcss src/client/css/index.css -o public/css/bundle.custom.css",
    "build-server": "babel ./src/server --out-dir ./dist/server && cp -r public dist/public && cp -r src/server/views dist/server/views",
    "clean": "rm -rf dist && mkdir -p dist",
    "css:components:watch": "nodemon --ext css --watch src/components --exec \"npm run build-css:components\"",
    "css:custom:watch": "nodemon --ext css --watch src/client/css --exec \"npm run build-css:custom\"",
    "dev": "run-p start-dev-server start-server css:components:watch css:custom:watch html:components:watch",
    "gen": "npm run generate-html-doc && npm run generate-react-doc && npm run generate-react-example-props-doc",
    "generate-html-doc": "babel-node ./scripts/generate-html-component-metadata.js",
    "generate-react-doc": "react-docgen ./src/components -e index.js -o component-metadata--react.json --pretty",
    "generate-react-example-props-doc": "babel-node ./scripts/generate-react-component-example-props.js",
    "html:components:watch": "nodemon --ext html --watch src/components --exec \"npm run generate-html-doc\"",
    "postinstall": "npm run build",
    "start-server": "babel-node src/server/server.js",
    "start-dev-server": "webpack-dev-server --config webpack-dev.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Telia Norge",
  "license": "ISC",
  "devDependencies": {
    "css-loader": "^0.28.4",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "style-loader": "^0.18.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "htmlencode": "0.0.4",
    "lodash": "^4.17.4",
    "marked": "^0.3.6",
    "pretty": "^2.0.0",
    "proxy-middleware": "^0.15.0",
    "postcss-cli": "^4.0.0",
    "postcss-easy-import": "^2.0.0",
    "postcss-loader": "^2.0.5",
    "postcss-nested": "^2.0.2",
    "react": "^15.5.4",
    "react-docgen": "^2.15.0",
    "react-dom": "^15.5.4",
    "react-element-to-jsx-string": "^8.0.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-router-dom": "^4.1.1",
    "webpack": "^2.6.1",
  }
}
