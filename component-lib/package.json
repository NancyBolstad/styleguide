{
  "name": "@telia/styleguide",
  "version": "1.18.0",
  "private": false,
  "description": "",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "scripts": {
    "build:library": "run-p build:library:css build:library:js && npm run svgo && npm run svg-join && npm run allicons:react",
    "build:library:css": "postcss src/index.pcss -o dist/index.css --config ./postcss.config.js",
    "build:library:js": "rollup --environment NODE_ENV:production -c rollup.build.js",
    "build:library:watch": "run-p build:library:css:watch build:library:js:watch svgo",
    "build:library:css:watch": "postcss src/index.pcss -o dist/index.css --watch --config ./postcss.config.js",
    "build:library:js:watch": "rollup --environment NODE_ENV:development -c rollup.build.js -w",
    "deploy:prod": "npm publish --access=public && babel-node ../deploy-slack.js component-lib",
    "lint": "eslint --ext .jsx,.js /",
    "prepublishOnly": "",
    "publish:local-test": "npm run build:library && npm pack",
    "svgo": "svgo -f ./assets/unoptimized-icons -o ./assets/icons --config=./svgo.config.yml --multipass",
    "svgo:pebbles": "svgo -f ./assets/pebbles -o ./assets/pebbles-optimized --config=./svgo.config.yml --multipass",
    "svg-join": "svg-join -s \"assets/icons/*.svg\" -o ./assets/allicons -n allicons  --mono",
    "allicons:react": "node generate-AllIcons-component.js && babel ./assets/allicons/AllIcons.jsx.generated --out-file ./assets/allicons/AllIcons.js"
  },
  "author": "Telia Norge",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "4.8.0",
    "eslint-plugin-react": "7.4.0",
    "npm-run-all": "4.1.1",
    "postcss": "6.0.22",
    "postcss-calc": "6.0.0",
    "postcss-cli": "4.1.1",
    "postcss-custom-properties": "6.1.0",
    "postcss-easy-import": "3.0.0",
    "postcss-functions": "3.0.0",
    "postcss-loader": "2.0.6",
    "postcss-nested": "^2.1.2",
    "postcss-replace": "^1.0.3",
    "postcss-svg": "^2.4.0",
    "postcss-url": "^7.3.0",
    "rollup": "^0.58.2",
    "rollup-copy-plugin": "0.0.1",
    "rollup-plugin-babel": "^4.0.1",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-size-snapshot": "^0.7.0",
    "rollup-plugin-uglify": "^3.0.0",
    "svg-join": "^1.1.1",
    "svg4everybody": "^2.1.9",
    "svgo": "^0.7.2"
  },
  "dependencies": {
    "classnames": "2.2.5"
  },
  "sideEffects": false,
  "peerDependencies": {
    "prop-types": "15.x",
    "react": "16.x",
    "react-dom": "16.x",
    "lodash-es": "^4.17.11"
  },
  "files": [
    "/dist",
    "/assets"
  ]
}
